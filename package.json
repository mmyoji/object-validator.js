{
  "name": "@mmyoji/object-validator",
  "version": "0.0.5",
  "description": "Zero dependency, simple object validator library",
  "keywords": [
    "validator",
    "validation"
  ],
  "homepage": "https://github.com/mmyoji/object-validator.js#readme",
  "bugs": {
    "url": "https://github.com/mmyoji/object-validator.js/issues"
  },
  "license": "MIT",
  "author": "mmyoji",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmyoji/object-validator.js.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "build": "vite build",
    "test": "vitest --run",
    "test:watch": "vitest",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.3"
  }
}
